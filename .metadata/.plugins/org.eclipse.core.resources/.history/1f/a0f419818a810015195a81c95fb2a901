
public class holoa {

	public static void main(String[] args) {
		
		
		int a = 100000;
		System.out.println(Integer.toBinaryString(100000));
		int b = a >> 24 & 0xff;
		System.out.println(Integer.toBinaryString(b));
		System.out.println(Integer.toBinaryString(050));
		System.out.println(a);
	}

}
