
# ==== Cloclk inputs (CLK) ====

NET "clk"   LOC = "B8"	| IOSTANDARD = "LVCMOS33";
NET "clk" TNM_NET = sys_clk;
TIMESPEC TS_sys_clk = PERIOD sys_clk_pin 50 MHz HIGH 50%; 

NET "rst"	LOC = "B18	"	| IOSTANDARD = "LVCMOS33";
NET "led"	LOC = "j14"	| IOSTANDARD = "LVCMOS33";

