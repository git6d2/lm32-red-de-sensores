NET "gpio_io<5>""	| IOSTANDARD = "LVCMOS33";		
NET "gpio_io<6>"	LOC = "L17"	| IOSTANDARD = "LVCMOS33";		
NET "gpio_io<7>"	LOC = "M15"	| IOSTANDARD = "LVCMOS33";		

